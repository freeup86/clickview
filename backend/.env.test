# Test Environment Configuration
NODE_ENV=test

# Test Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME_TEST=clickview_test
DB_USER=clickview_user
DB_PASSWORD=password
DB_SSL=false

# Test Redis (use different DB number)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_TEST_DB=1

# Security (test keys - not secure)
JWT_SECRET=test_jwt_secret_not_for_production_use_only_testing
JWT_REFRESH_SECRET=test_refresh_secret_not_for_production
ENCRYPTION_KEY=test_encryption_key_32_chars_long
SESSION_SECRET=test_session_secret_not_for_production

# Disable features for testing
EMAIL_ENABLED=false
MFA_ENABLED=false
AUDIT_LOG_ENABLED=true

# Logging (minimal for tests)
LOG_LEVEL=error
LOG_TO_FILE=false

# Disable rate limiting for tests
RATE_LIMIT_ENABLED=false
