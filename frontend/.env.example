# Backend API Configuration
VITE_API_URL=http://localhost:5000/api

# Application Configuration
VITE_APP_NAME=ClickView
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

# GraphQL Configuration (if using GraphQL)
VITE_GRAPHQL_URL=http://localhost:5000/graphql

# WebSocket Configuration (for future real-time features)
VITE_WS_URL=ws://localhost:5000

# Authentication
VITE_AUTH_TOKEN_KEY=clickview_token
VITE_AUTH_REFRESH_TOKEN_KEY=clickview_refresh_token

# ClickUp OAuth (if implementing OAuth flow)
VITE_CLICKUP_CLIENT_ID=your_clickup_client_id
VITE_CLICKUP_REDIRECT_URI=http://localhost:3000/auth/clickup/callback

# Feature Flags
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_TRACKING=false

# Analytics (Optional)
VITE_GA_TRACKING_ID=
VITE_MIXPANEL_TOKEN=

# Error Tracking (Optional)
VITE_SENTRY_DSN=

# Development Settings
VITE_ENABLE_DEV_TOOLS=true
VITE_LOG_LEVEL=debug

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true

# API Timeouts (milliseconds)
VITE_API_TIMEOUT=30000
VITE_UPLOAD_TIMEOUT=120000

# Cache Configuration
VITE_CACHE_ENABLED=true
VITE_CACHE_TTL=300000

# Pagination
VITE_DEFAULT_PAGE_SIZE=10
VITE_MAX_PAGE_SIZE=100

# File Upload
VITE_MAX_UPLOAD_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/*,application/pdf

# Map/Chart Configuration
VITE_MAP_API_KEY=
VITE_CHART_ANIMATION_DURATION=300

# Support & Help
VITE_SUPPORT_EMAIL=support@clickview.com
VITE_HELP_URL=https://docs.clickview.com

# Social Links
VITE_GITHUB_URL=
VITE_TWITTER_URL=
VITE_SLACK_URL=
